+help:
+	@echo ""
+	@echo "           CLIPS Makefile"
+	@echo ""
+	@echo "Usage: "
+	@echo ""
+	@echo " * Build clips:                   'make clips'"
+	@echo " * Build libraries:               'make libs'"
+	@echo " * Build both, clips & libraries: 'make all'"
+	@echo " * Install all:                   'make install'"
+	@echo " * Install libraries only:        'make install-libs'"
+	@echo " * Install clips:                 'make install-clips'"
+	@echo " * Clean sources:                 'make clean'"
+	@echo " * Full clean:                    'make distclean'"
+	@echo ""
+	@echo " You can build clips statically linked to clips library doing:"
+	@echo ""
+	@echo "        configure --enable-static-clips && make clips"
+	@echo ""
+
+
+
+all: libs $(CLIPS_NAME)
+
+EXTRAOBJS   +=  $(srcdir)/main.o $(srcdir)/userfunctions.o
+CLIPS_NAME = clips 
+
+ifeq ($(WANT_EMACS_EDITOR),1)
+EXTRAOBJS += $(EDSOURCES::.c=.o)
+endif
+info: clips.info
+dvi: clips.dvi
+
+%.o: %.c
+	$(CC) -c $(CPPFLAGS) $(DEFS) -fPIC $(CFLAGS) $<
+
+install: installdirs install-bin install-libs install-includes
+
+installdirs:
+	${srcdir}/mkinstalldirs $(DESTDIR)$(bindir) $(DESTDIR)$(infodir)
+
+install-bin: $(CLIPS_NAME)
+	$(INSTALL_PROGRAM) clips $(DESTDIR)$(bindir)/$(binprefix)clips
+
+#install-data: clips.info
+#	-$(INSTALL_DATA) $(srcdir)/clips.info $(DESTDIR)$(infodir)/clips.info
+
+# Like install, but striping binaries.
+install-strip :
+	$(MAKE) DESTDIR='$(DESTDIR)' INSTALL_PROGRAM='$(INSTALL_PROGRAM) -s' \
+	                install
+
+# Install libraries into standard location.
+install-libs : libs
+	$(INSTALL_PROGRAM) -s libclips.a $(DESTDIR)$(libdir)/
+	$(INSTALL_PROGRAM) -s libclips.so.$(VERSION) $(DESTDIR)$(libdir)/
+	cp -a libclips.so.$(VERSION_MAJOR) $(DESTDIR)$(libdir)/
+	cp -a libclips.so                  $(DESTDIR)$(libdir)/
+#		-ldconfig -vl $(DESTDIR)$(libdir)/libclips.so.$(VERSION)
+
+install-includes :
+	@if [ -d $(DESTDIR)$(includedir)/clips ]; then true; \
+	else mkdir $(DESTDIR)$(includedir)/clips; \
+	fi
+	@echo "Installing headers in $(DESTDIR)$(includedir)"
+	@for file in $(INSTHEADERS); do \
+	echo "Installing header: $${file}"...; \
+	$(INSTALL_DATA) $${file} $(DESTDIR)$(includedir)/clips/$${file}; \
+	done
+
+ifdef WANT_STATIC_CLIPS
+$(CLIPS_NAME) : depend $(OBJS)
+	$(CC) $(CPPFLAGS) $(CFLAGS) $(LDFLAGS) $(LIBS) $(OBJS) -o $@
+else
+$(CLIPS_NAME) : $(EXTRAOBJS) libclips.so
+	$(CC) $(CPPFLAGS) $(CFLAGS) $(LDFLAGS) $(EXTRAOBJS) -lclips -o $@
+endif
+
+#clips: depend $(OBJS)
+#	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(LIBS)
+
+clips.o getopt.o getopt1.o: getopt.h
+
+clips.info: clips.texi
+	$(MAKEINFO) -I$(srcdir) -o clips.info $(srcdir)/clips.texi
+clips.dvi: clips.texi
+	$(TEXI2DVI) $(srcdir)/clips.texi
+
+libs : libclips.a libclips.so
+
+libclips.a : depend $(LIBOBJS)
+	$(AR) $(ARFLAGS) $@ $(LIBOBJS)
+	$(RANLIB) $@
+
+libclips.so : depend $(LIBOBJS)
+	$(CC)  -fPIC -shared -Wl,-soname,$@.$(VERSION_MAJOR) -o $@.$(VERSION) $(LIBOBJS) $(LIBS)
+	$(LN_S) -sf $@.$(VERSION) $@.$(VERSION_MAJOR)
+	$(LN_S) -sf $@.$(VERSION) $@
+
+TAGS: $(SRCS)
+	etags $(SRCS)
+
+
+clean:
+	-$(RM) -f confdefs* conftest* core core.* *.core *~ *.d *.o *.output \
+	     *.tab.* DEADJOE \\#*\\# \
+
+mostlyclean: clean
+
+distclean: clean
+	-$(RM) -f Makefile clips clips.dvi libclips.a libclips.so* \
+	clips_config.h config.log config.status config.cache .tmpdepend
+
+realclean: distclean
+	-$(RM) -f TAGS hello.info*
+
+maintainer-clean : distclean
+
+realclean: distclean
+
+dist: $(DISTFILES)
+	echo clips-`sed -e '/version/!d' -e 's/[^0-9.]*\([0-9.]*\).*/\1/' -e q version.c` > .fname
+	rm -rf `cat .fname`
+	mkdir `cat .fname`
+	ln $(DISTFILES) `cat .fname`
+	tar chozf `cat .fname`.tar.z `cat .fname`
+	rm -rf `cat .fname` .fname
+
+.PHONY: help all libs clean mostlyclean distclean realclean maintainer-clean realclean depend dep
+
+check: clips
+# 	To be done...
+#	@echo expect no output from diff
+#	./hello > test.out
+#	diff -c $(srcdir)/testdata test.out
+#	rm -f test.out
+
+.tmpdepend : $(SOURCES)
+	   $(CC) -MM $(CFLAGS) $(SOURCES) > .tmpdepend
+
+depend : .tmpdepend
+
+dep : depend
+
+ifeq (.tmpdepend,$(wildcard .tmpdepend))
+include .tmpdepend
+endif
+
+# Prevent GNU make v3 from overflowing arg limit on SysV.
+.NOEXPORT:
